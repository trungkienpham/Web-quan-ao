@model List<Models_OnlineShop.EF.Tag>
<div class="tag">
    <h3 class="cate">Tags</h3>
    <div class="tags">
        <ul>
            @foreach (var item in Model)
            {
                var url = "/nhan/" + item.Name + "-" + item.ID+"-"+3;
                <li><a href=@url>@item.Name</a></li>
            }         
            <div class="clearfix"> </div>
        </ul>
    </div>
</div>
